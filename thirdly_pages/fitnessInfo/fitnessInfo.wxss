/* 进度条的颜色 */
.wx-progress-inner-bar{
  border-radius: 8rpx;
background: linear-gradient(-90deg, #FFD366, #28CF97, #FF8B66 ) !important;
}
.wx-progress-inner-bar {  border-radius: 10px !important}
.title{
  position: fixed;
  display: flex;
  top: 0;
  align-items: center;
  /* justify-content: center; */
  width: 100vw;
}
.title  view{
margin: 0 8rpx;
font-size:34rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;

}
.goback {
  margin-left: 14rpx;
}
.goback image{
  width: 50rpx;
  height: 50rpx;
}
.top{
  margin-top: 196rpx;
  text-align: center;
}
.top .slefimg,.userphoto{
  width: 181rpx;
  height: 181rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 auto;
}
.slefFirstname{
  width: 181rpx;
  height: 181rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 56rpx;
  color: white;
  font-weight: 500;
  background: rgba(48,122,245,1);

}
.username{
font-size: 36rpx;
font-family: PingFang SC;
font-weight: bold;
color: #343434;
}
.together{
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin: 51rpx 0 38rpx 0;
}
.together > view{
  width: 207rpx;
height: 141rpx;
border-radius: 8rpx;
text-align: center;
padding-top: 33rpx;
}
.together > view text{
font-size: 44rpx;
font-family: PingFang SC;
font-weight: bold;
color: #FFFFFF;
}
.together > view view{
font-size: 24rpx;
font-family: PingFang SC;
font-weight: 500;
color: #FFFFFF;
}
.together .togeth_v1{
background: linear-gradient(-70deg, #82BAFF, #5877FF);
box-shadow: 0px 8rpx 16rpx 0px rgba(94, 128, 255, 0.4);
}
.together .togeth_v2{
background: linear-gradient(135deg, #90F7EC, #32CCBC);
box-shadow: 0px 8rpx 16rpx 0px rgba(47, 196, 180, 0.4);
}
.together .togeth_v3{
background: linear-gradient(135deg, #FFF6B7, #F6416C);
box-shadow: 0px 8rpx 16rpx 0px rgba(247, 80, 114, 0.4);
}
