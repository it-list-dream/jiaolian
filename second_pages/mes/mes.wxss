.line {
  height: 2rpx;
  width: 100%;
  background-color: rgba(244, 244, 244, 1);
  margin-bottom: 28rpx;
}

.fiexd{
  background: #fff;
  width:100%;
  position: fixed
}
.navbar {
  padding: 0 48rpx;
  display: flex;
  justify-content: space-between;
}
.navbar image {
  width: 34rpx;
  height: 22rpx;
  margin-bottom: 12rpx;
}
.navbar .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFangSC;
  font-weight: 500;
  color: rgba(118, 129, 150, 1);
  padding-bottom: 32rpx;
}
.navbar .item.active {
  color: #307af5;
  font-size: 30rpx;
}

.info {

  background-color: white;
}

.info .info-con {
  padding: 0 50rpx;
}

.info .info-con .info-flex {
  display: flex;
  align-items: center;
  height: 160rpx;
  border-bottom: 1px solid rgba(230, 234, 241, 1);
}

.info .info-con .info-flex .user {
  margin-left: 48rpx;
  min-width: 60%;

}
.info .info-con .info-flex .user .user-info{
  display: flex;
   flex-wrap: wrap ;
  justify-content: flex-start;
 align-items: center
}
.info .info-con .info-flex .user .user-info view:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(13, 14, 21, 1);
  max-width: 170rpx;
overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}

.info .info-con .info-flex .user .user-info view:nth-child(2) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: #307AF5;
}

.info .info-con .info-flex .user > view:nth-child(2) {
  margin-top: 2rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(118, 129, 150, 1);
}

.info .info-con .info-flex .phone {
 margin-left: auto;
width: 10%;
height: 100%;
display: flex;
justify-content: flex-end;
align-items: center;

}

.info .info-con .info-flex .phone image {
  width: 42rpx;
  height: 42rpx;
}

.info .info-con .head-img {
 width: 88rpx;
  height: 88rpx;
   border-radius: 50%; 
 
 
}
.info .info-con .notimage {
  width: 88rpx;
  height: 88rpx;
  background: rgba(65, 128, 255, 1);
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  font-size: 44rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

/* 打电话模态框 */

.modal2 {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}

.phonemodel {
   position: fixed;
  top: 43%;
  left: 10%;
  width:80%;
height:321rpx;
background:rgba(255,255,255,1);
border-radius:16rpx;
z-index: 1001;
}

.phonemodel >view:nth-child(1) {
  height: 54rpx;
  width: 54rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: auto;
}

.phonemodel image {
  width: 24rpx;
  height: 24rpx;
}

.phonemodel .phonemodelinfo {
  padding: 0 80rpx;
}

.phonemodel .phonemodelinfo view {
  height: 112rpx;
  display: flex;
  align-items: center;
  justify-content: center;

}

.phonemodel .phonemodelinfo view:first-child{
 border-bottom: 2rpx solid rgba(230,234,241,1);
}
.phonemodel > view:nth-child(3) {
  width: 196rpx;
  height: 64rpx;
  background: rgba(48, 122, 245, 1);
  border-radius: 4rpx;
  border: 2rpx solid rgba(48, 122, 245, 1);
  margin: 40rpx auto 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

/* 即将到期 */

.upcom .user-info text:nth-child(2) {
  color: #307AF5 !important;
}

.upcom .user-infob text {
  color: #768196;
}
