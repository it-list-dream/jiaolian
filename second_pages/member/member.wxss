.page {
  width: 100%;
}

.line {
  height: 2rpx;
  width: 100%;
  background-color: rgba(244, 244, 244, 1);
  margin-bottom: 28rpx;
}

.bot-line {
  width: 64rpx;
  height: 6rpx;
  background: rgba(48, 122, 245, 1);
  border-radius: 100px 100px 0px 0px;
  margin-top: 4rpx;
}

.navbar {
  margin-left: -10%;
  width: 320rpx;
  display: flex;
  justify-content: space-around;
}

.navbar .item {
  height: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 34rpx;
  font-weight: 800;
  font-family: PingFang-SC-Heavy;
  margin: 0 2rpx;
}

.navbar .item.active {
  color: rgba(48, 122, 245, 1);
}

/* 意向会员 */

.purpose-search {
  position: relative;
  display: flex;
  justify-content: space-between;
  padding: 0 48rpx;
}

.purpose-search .search {
  display: flex;
  align-items: center;
  width: 528rpx;
  height: 62rpx;
  background: rgba(230, 234, 241, 0.5);
  border-radius: 12rpx;
}

.purpose-search .search .sousuo {
  height: 32rpx;
  width: 32rpx;
  margin-left: 32rpx;
}

.purpose-search .search input {
  margin-left: 10rpx;
  width: 400rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgb(118, 129, 150);
  opacity: 0.5;
}

.purpose-search .search .clear {
  width: 100rpx;
  height: 50rpx;
}

.purpose-search .search .clear image {
  width: 32rpx;
  height: 32rpx;
  margin-top: 9rpx;
  margin-left: 50rpx;
}

.purpose-search .chose {
  width: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.purpose-search .chose image {
  height: 8rpx;
  width: 14rpx;
  margin-right: auto;
  margin-left: 12rpx;
}

.purpose-search .chose text {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.purpose .purpose-info, .card .purpose-info {
  margin-top: 32rpx;
  padding: 0 50rpx;
  min-height: 1000rpx;
}

.purpose .purpose-info .info-flex, .card .purpose-info .info-flex {
  display: flex;
  align-items: center;
  height: 160rpx;
  border-bottom: 1px solid rgba(230, 234, 241, 1);
}

.purpose .purpose-info .info-flex .user, .card .purpose-info .info-flex .user {
  margin-left: 48rpx;
  min-width: 60%;
}

.card .purpose-info .info-flex .user .user-info  .userCard {
  text-align: center;
  width: 37px;
  height: 20px;
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(118, 129, 150, 0.7);
  line-height: 20px;
}

.card .purpose-info .info-flex .user .user-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.purpose .purpose-info .info-flex .user .user-info text:nth-child(1),
.card .purpose-info .info-flex .user .user-info view:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(13, 14, 21, 1);
  max-width: 170rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.purpose .purpose-info .info-flex .user .user-info text:nth-child(2),
.card .purpose-info .info-flex .user .user-info view:nth-child(2) {
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(48, 122, 245, 1);
}

.purpose .purpose-info .info-flex .user > view:nth-child(2),
.card .purpose-info .info-flex .user > view:nth-child(2) {
  margin-top: 2rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(118, 129, 150, 1);
}

.purpose .purpose-info .info-flex .phone, .card .purpose-info .info-flex .phone {
  margin-left: auto;
  height: 100%;
  display: flex;
  align-items: center;
}

.purpose .purpose-info .info-flex .phone image,
.card .purpose-info .info-flex .phone image {
  width: 42rpx;
  height: 42rpx;
}

.purpose .purpose-info .head-img, .card .purpose-info .head-img {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}

.purpose .purpose-info  .Nofind, .card .purpose-info  .Nofind {
  height: 500rpx;
  width: 654rpx;
  text-align: center;
  line-height: 500rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

/* 意向会员筛选 */

.model {
  position: absolute;
  top: 600rpx;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 0;
}

.model-info {
  position: absolute;
  top: 296rpx;
  z-index: 1000;
  background: white;
  width: 100%;
  height: 702rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 0 0 32rpx 32rpx;
  padding: 32rpx 48rpx 0 48rpx;
}

.model-info .blubg {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #4d75fa;
  border: 1px solid #4d75fa;
}

.model-info .grabg {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  border: 1px solid rgba(153, 153, 153, 1);
}

.model-info .blu {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(48, 122, 245, 1);
  margin-left: 20rpx;
}

.model-info .gra {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-left: 20rpx;
}

.chose-num input {
  width: 64px;
  height: 32px;
  background: rgba(230, 234, 241, 0.7);
  border-radius: 6px;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(118, 129, 150, 0.7);
}

.chose-num {
  display: flex;
  align-items: center;
  margin-left: auto;
}

.chose-num .te1 {
  margin: 0 20rpx;
}

.chose-num .te2 {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-left: 32rpx;
}

.model-info .yet-follow, .bg3, .bg22 {
  display: flex;
  align-items: center;
}

.model-info .kong-line {
  width: 100%;
  height: 2rpx;
  background: rgba(230, 234, 241, 1);
  margin: 32rpx 0 18rpx 0;
}

.model-info .kong-line2 {
  width: 100%;
  height: 2rpx;
  background: rgba(230, 234, 241, 1);
  margin-top: 18rpx;
}

.model-info .inquire {
  width: 100%;
  height: 88rpx;
  background: rgba(48, 122, 245, 1);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  margin-top: 195rpx;
}

.model-info .inquire1 {
  width: 100%;
  height: 88rpx;
  background: rgba(48, 122, 245, 1);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  margin-top: 125rpx;
}

/* 打电话模态框 */

.modal2 {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}

.phonemodel {
  position: fixed;
  top: 30%;
  left: 10%;
  width: 80%;
  height: 450rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  z-index: 1001;
}

.phonemodel >view:nth-child(1) {
  height: 54rpx;
  width: 54rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: auto;
}

.phonemodel .imgview {
  height: 50rpx;
  width: 50rpx;
}

.phonemodel image {
  width: 32rpx;
  height: 32rpx;
}

.phonemodel .phonemodelinfo {
  padding: 0 80rpx;
}

.phonemodel .phonemodelinfo view {
  height: 112rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 2rpx solid rgba(230, 234, 241, 1);
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.phonemodel > view:nth-child(3) {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 32rpx;
}

.notimage {
  width: 88rpx;
  height: 88rpx;
  background: rgba(65, 128, 255, 1);
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  font-size: 44rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

/* 返回顶部 */

.goTop {
  height: 92rpx;
  width: 92rpx;
  position: fixed;
  bottom: 200rpx;
  right: 28rpx;
  border-radius: 50%;
}

.botEnd {
  text-align: center;
  margin: 30rpx 0;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}
