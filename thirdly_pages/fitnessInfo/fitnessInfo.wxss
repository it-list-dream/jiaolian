  align-items: center;
  /* justify-content: space-between; */
 
  flex-wrap: wrap;
}
.indicatorInfo_con .conditionList text{
font-size: 26rpx;
font-family: PingFang SC;
font-weight: 500;
color: #606060;

}
.indicatorInfo_con .conditionList{
  width: 218rpx;
height: 64rpx;
border: 1rpx solid #606060;
border-radius: 8rpx;
