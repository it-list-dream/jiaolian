  background: white;
  width: 100vw;
  padding: 40rpx 0 40rpx 30rpx;
}
.indicator .indicatorInfo .indicatorInfo_titile image{
   width: 30rpx;
   height: 30rpx;
}
.indicator .indicatorInfo .indicatorInfo_titile text{
