/*弹性布局*/

 .flex {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
} 
 
.line1 {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
 
.commonHead {
  width: 100%;
  height: 125rpx;
 
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  overflow: hidden;
  position: relative;
  z-index: 10;
  /* border-bottom: 1px solid black */
  /* padding-bottom:20px; 
    border-bottom: 10px solid red */
}
 
.commonHead-wrap {
  width: 100%;
  position: absolute;
  overflow: hidden;
  left: 0;
  bottom: -5rpx;
  height: 45px;
  line-height: 45px;
}
.commonHead_left {
  width: 100rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0
}
                  
.commonHead_left_back {
  width: 100%;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-size: 40rpx 40rpx;
  background-repeat: no-repeat;
  background-position: center;
}
.commonHead_right {
  width: 100%;
  height: 100%;
  line-height: 88rpx;
  text-align: center;
  font-size: 34rpx;
  overflow: hidden;
  justify-content:flex-end;
  font-weight: 600;
}
 
.commonHead_right_text {
  width: 280rpx;
  margin: 0 auto;
  font-family: PingFangSC-Medium, sans-serif;

}